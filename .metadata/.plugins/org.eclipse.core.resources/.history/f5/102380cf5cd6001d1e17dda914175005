package Dio.gof;


/**
 * Singleton "preguiçoso"
 * @author aliss
 *
 */

public class SingletonEager {
	
	private static SingletonEager instancia;
	
	private SingletonEager() {
		super();
	}
	
	private static SingletonEager getInstancia() {
		if (instancia == null) {
			instancia = new SingletonEager();
		}
		return instancia;
	}

}
